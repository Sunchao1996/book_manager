<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bl.dao.LoanDao">
	<insert id="insert">
		insert into borrow_info(borrowId,readerId,borrowTime,backTime,returnTime,borrowStatus,bookSeat,bookBarCode)
		values
		(
		#{borrowId},#{readerId},#{borrowTime},#{backTime},#{returnTime},#{borrowStatus},#{bookSeat},#{bookBarCode}
		)
	</insert>
</mapper>